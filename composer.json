{
  "name": "telegram-bot-native-php/telegram-bot-native-php",
  "description": "The Telegram Bot on native PHP",
  "require": {
    "php": "^8.1",
    "composer/installers": "^1.9",
    "telegram-bot/api": "v2.5.0",
    "ext-pdo": "*"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "allow-plugins": {
      "composer/installers": true
    }
  }
}
